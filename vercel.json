{


  "version": 2,

  "builds": [

    {


      "src": "package.json",


      "use": "@vercel/static-build",


      "config": {


        "distDir": "public"


      }


      "src": "index.html",


      "use": "@vercel/static"

    }

  ],

  "routes": [


    {


      "src": "/(.*)",


      "dest": "/public/$1"


    }


    { "handle": "filesystem" },


    { "src": ".*", "dest": "index.html" }

  ]

}
