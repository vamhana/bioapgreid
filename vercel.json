{
  "version": 2,
  "routes": [
    {
      "src": "/app/core/(.*)",
      "dest": "/app/core/$1.js"
    },
    {
      "src": "/app/(.*)",
      "dest": "/app/$1.js"
    },
    {
      "src": "/modules/(.*)",
      "dest": "/modules/$1.js"
    },
    {
      "src": "/app/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": false
    },
    {
      "src": "/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "src": "/(.*)\\.mjs",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/results/(.*)\\.json",
      "headers": {
        "Content-Type": "application/json"
      },
      "continue": true
    },
    {
      "src": "/app/(.*)",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
