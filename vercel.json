{
  "version": 2,
  "builds": [
    {
      "src": "bioapgreid/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bioapgreid/public/$1"
    }
  ],
  "buildCommand": "cd bioapgreid && npm run build"
}
