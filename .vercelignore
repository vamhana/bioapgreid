    <!-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø—Ä–∏–ª–æ–∂–µ–Ω–∏—è -->
    <script>
        // –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏ DOM –∏ –∫—Ä–∏—Ç–∏—á–µ—Å–∫–∏—Ö —Ä–µ—Å—É—Ä—Å–æ–≤
        document.addEventListener('DOMContentLoaded', function() {
            console.log('üöÄ –ó–∞–ø—É—Å–∫ –ì–∞–ª–∞–∫—Ç–∏–∫–∏ GENO–§–û–ù–î v2.1 –Ω–∞ bioapgreid.ru...');
            
            // –û–±–Ω–æ–≤–ª—è–µ–º –º–µ—Ç–∞–¥–∞–Ω–Ω—ã–µ –∑–∞–≥—Ä—É–∑–∫–∏
            const startTime = Date.now();
            window.genofondAnalytics.domReady = startTime;
            
            // –°–æ–∑–¥–∞–µ–º –∏ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–µ–º –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ
            const app = new GenofondApp();
            window.genofondApp = app; // –ì–ª–æ–±–∞–ª—å–Ω–∞—è —Å—Å—ã–ª–∫–∞ –¥–ª—è –æ—Ç–ª–∞–¥–∫–∏
            
            // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –∑–∞–≥—Ä—É–∑–∫–∏
            function updateLoaderStats(components, resources, message) {
                const componentsEl = document.getElementById('loaderComponents');
                const resourcesEl = document.getElementById('loaderResources');
                const textEl = document.getElementById('preloaderText');
                const percentEl = document.getElementById('preloaderPercent');
                const progressEl = document.getElementById('preloaderProgress');
                const timeEl = document.getElementById('loaderTime');
                
                if (componentsEl) componentsEl.textContent = components;
                if (resourcesEl) resourcesEl.textContent = resources + '%';
                if (textEl) textEl.textContent = message;
                if (percentEl) percentEl.textContent = resources + '%';
                if (progressEl) progressEl.style.width = resources + '%';
                if (timeEl) {
                    const elapsed = (Date.now() - startTime) / 1000;
                    timeEl.textContent = elapsed.toFixed(1) + 's';
                }
            }
            
            // –ü–æ–¥–ø–∏—Å–∫–∞ –Ω–∞ —Å–æ–±—ã—Ç–∏—è –∑–∞–≥—Ä—É–∑–∫–∏
            document.addEventListener('appInitialized', function() {
                console.log('‚úÖ –ì–∞–ª–∞–∫—Ç–∏–∫–∞ GENO–§–û–ù–î —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω–∞ –Ω–∞ bioapgreid.ru!');
                window.genofondAnalytics.appReady = Date.now();
                
                // –°–∫—Ä—ã–≤–∞–µ–º –ø—Ä–µ–ª–æ–∞–¥–µ—Ä —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
                const preloader = document.getElementById('preloader');
                if (preloader) {
                    preloader.style.opacity = '0';
                    setTimeout(() => {
                        preloader.style.display = 'none';
                        document.body.setAttribute('data-loading', 'false');
                    }, 500);
                }
                
                // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø—Ä–∏–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ–µ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
                if (app.showNotification) {
                    app.showNotification('–ì–∞–ª–∞–∫—Ç–∏–∫–∞ GENO–§–û–ù–î –≥–æ—Ç–æ–≤–∞ –∫ –∏—Å—Å–ª–µ–¥–æ–≤–∞–Ω–∏—é!', 'success');
                }
            });
            
            document.addEventListener('componentLoaded', function(e) {
                const { component, time } = e.detail;
                const componentsLoaded = document.querySelectorAll('[data-component-loaded]').length + 1;
                const progress = Math.min(95, Math.round((componentsLoaded / 8) * 100));
                
                updateLoaderStats(
                    `${componentsLoaded}/8`,
                    progress,
                    `–ó–∞–≥—Ä—É–∑–∫–∞ ${component}...`
                );
            });
            
            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏
            app.init().catch(error => {
                console.error('‚ùå –û—à–∏–±–∫–∞ –∑–∞–ø—É—Å–∫–∞ –≥–∞–ª–∞–∫—Ç–∏–∫–∏:', error);
                window.genofondAnalytics.initError = error.message;
                
                // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ–± –æ—à–∏–±–∫–µ
                const preloader = document.getElementById('preloader');
                if (preloader) {
                    preloader.innerHTML = `
                        <div class="error-state">
                            <div class="error-icon" aria-hidden="true">üí•</div>
                            <h3 class="error-title">–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ –≥–∞–ª–∞–∫—Ç–∏–∫–∏</h3>
                            <p class="error-message">${error.message}</p>
                            <div class="error-actions">
                                <button class="error-btn primary" onclick="location.reload()">
                                    –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∏—Ç—å
                                </button>
                                <button class="error-btn secondary" onclick="window.genofondApp.enterSafeMode()">
                                    –ë–µ–∑–æ–ø–∞—Å–Ω—ã–π —Ä–µ–∂–∏–º
                                </button>
                                ${window.genofondConfig.environment === 'development' ? 
                                `<button class="error-btn tertiary" onclick="this.nextElementSibling.style.display='block'">
                                    –ü–æ–¥—Ä–æ–±–Ω–æ—Å—Ç–∏
                                </button>
                                <div class="error-details" style="display:none">
                                    <pre>${error.stack}</pre>
                                </div>` : ''}
                            </div>
                        </div>
                    `;
                }
            });
            
            // –†–∞–Ω–Ω—è—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤
            updateLoaderStats('0/8', '0', '–ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–≥–æ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–∞...');
        });
        
        // –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ –∑–∞–≥—Ä—É–∑–∫–∏ —Ä–µ—Å—É—Ä—Å–æ–≤
        window.addEventListener('error', function(e) {
            if (e.target.tagName === 'SCRIPT' || e.target.tagName === 'LINK') {
                console.error('‚ùå –û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ —Ä–µ—Å—É—Ä—Å–∞:', e.target.src || e.target.href);
                if (window.genofondApp && window.genofondApp.showNotification) {
                    window.genofondApp.showNotification('–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ —Ä–µ—Å—É—Ä—Å–æ–≤', 'error');
                }
            }
        });
        
        // –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è –ø—Ä–∏ –∑–∞–∫—Ä—ã—Ç–∏–∏
        window.addEventListener('beforeunload', function() {
            if (window.genofondApp) {
                window.genofondApp.saveAppState();
            }
        });
    </script>
